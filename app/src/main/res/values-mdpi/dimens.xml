<resources>
    <!-- Example customization of dimensions originally defined in res/values/dimens.xml
         (such as screen margins) for screens with more than 820dp of available width. This
         would include 7" and 10" devices in landscape (~960dp and ~1280dp respectively). -->
    <!--Image View-->
    <dimen name="view_size_0">4dp</dimen>
    <dimen name="view_size_1">6dp</dimen>
    <dimen name="view_size_2">10dp</dimen>
    <dimen name="view_size_3">18dp</dimen>
    <dimen name="view_size_4">25dp</dimen>
    <dimen name="view_size_5">33dp</dimen>
    <dimen name="view_size_6">42dp</dimen>
    <dimen name="view_size_7">48dp</dimen>
    <dimen name="view_size_8">75dp</dimen> <!-- NOT CHANGE VALUE -->
    <dimen name="view_size_9">95dp</dimen>
    <dimen name="view_size_10">125dp</dimen>
    <dimen name="view_size_11">155dp</dimen>
    <dimen name="view_size_12">185dp</dimen>

    <!--Text size-->
    <dimen name="text_size_1">4sp</dimen>
    <dimen name="text_size_2">6sp</dimen>
    <dimen name="text_size_3">9sp</dimen>
    <dimen name="text_size_4">12sp</dimen>
    <dimen name="text_size_5">15sp</dimen>
    <dimen name="text_size_6">18sp</dimen>
    <dimen name="text_size_7">23sp</dimen>
    <dimen name="text_size_8">28sp</dimen>
    <dimen name="text_size_9">33sp</dimen>
    <dimen name="text_size_11">53sp</dimen>

    <!--Image size-->
    <item name="image_size_smaller" format="float" type="dimen">30</item>
    <item name="image_size_small" format="float" type="dimen">100</item>
    <item name="image_size_normal" format="float" type="dimen">250</item>
    <item name="image_size_big" format="float" type="dimen">400</item>
    <item name="image_size_bigger" format="float" type="dimen">700</item>
</resources>